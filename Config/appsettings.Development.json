{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
      "Endpoints": {
        "Https": {
          "Url": "https://localhost:5001",
          "ServerOptions": {
            "Certificate": {
              "File": "/certs/fullchain.pem",
              "Key": "/certs/privkey.pem"
            }
          }
        }
      }
  }
}
